{
  "name": "Everest Night Theme",
  "dark": true,
  "author": "Vedant Sahu <vedant.sahu2001@gmail.com>",
  "editorScheme": "/theme/scheme/everest-night-schema.xml",
  "colors": {
    "background": "#0e1622",
    "altBackground": "#171717",
    "border": "#8fa0b622",
    "buttonBackground": "#934164",
    "foreground": "#acc6ed",
    "hoverBackground": "#8fa0b60f",
    "inactiveForeground": "#8fa0b6",
    "secondaryButtonBackground": "#3B1C2D",
    "selectionBackground": "#8fa0b644"
  },
  "ui": {
    "*": {
      "background": "background",
      "borderColor": "border",
      "disabledBackground": "background",
      "disabledText": "inactiveForeground",
      "foreground": "foreground",
      "iconColor": "inactiveForeground",
      "inactiveBackground": "background",
      "inactiveForeground": "inactiveForeground",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "foreground",
      "selectionInactiveForeground": "foreground",
      "separatorColor": "selectionBackground",
      "windowBorder": "border"
    },
    "ActionButton": {
      "hoverBackground": "hoverBackground",
      "hoverBorderColor": "border",
      "pressedBackground": "border",
      "pressedBorderColor": "selectionBackground"
    },
    "Borders.color": "selectionBackground",
    "Button": {
      "endBackground": "buttonBackground",
      "endBorderColor": "buttonBackground",
      "focusedBorderColor": "buttonBackground",
      "shadowColor": "buttonBackground",
      "startBackground": "buttonBackground",
      "startBorderColor": "buttonBackground",
      "disabledBorderColor": "buttonBackground",
      "default": {
        "endBackground": "secondaryButtonBackground",
        "endBorderColor": "secondaryButtonBackground",
        "focusedBorderColor": "secondaryButtonBackground",
        "shadowColor": "secondaryButtonBackground",
        "startBackground": "secondaryButtonBackground",
        "startBorderColor": "secondaryButtonBackground"
      }
    },
    "CheckBox": {
      "background": "background",
      "disabledText": "inactiveForeground",
      "foreground": "foreground"
    },
    "ComboBox": {
      "ArrowButton": {
        "background": "background",
        "disabledIconColor": "inactiveForeground",
        "iconColor": "foreground",
        "nonEditableBackground": "background"
      },
      "background": "background",
      "foreground": "foreground",
      "modifiedItemForeground": "inactiveForeground",
      "nonEditableBackground": "background"
    },
    "CompletionPopup": {
      "selectionBackground": "altBackground",
      "selectionInactiveBackground": "altBackground",
      "matchForeground": "altBackground"
    },
    "Component": {
      "arc": "7",
      "borderColor": "border",
      "disabledBorderColor": "inactiveBackground",
      "errorFocusColor": "#FFB3B3",
      "focusColor": "border"
    },
    "Editor": {
      "background": "background",
      "foreground": "foreground"
    },
    "EditorPane": {
      "background": "background",
      "foreground": "foreground",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "foreground"
    },
    "Link": {
      "activeForeground": "inactiveForeground",
      "hoverForeground": "foreground",
      "pressedForeground": "inactiveForeground"
    },
    "Panel.background": "background",
    "ProgressBar": {
      "progressColor": "buttonBackground",
      "trackColor": "border"
    },
    "RadioButton": {
      "background": "background",
      "foreground": "foreground"
    },
    "TabbedPane": {
      "background": "background",
      "focusColor": "selectionBackground",
      "foreground": "foreground",
      "hoverColor": "hoverBackground"
    },
    "Table": {
      "background": "background",
      "foreground": "foreground",
      "gridColor": "border",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "foreground",
      "stripeColor": "hoverBackground"
    },
    "TextField": {
      "background": "background",
      "foreground": "foreground",
      "inactiveForeground": "inactiveForeground",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "foreground"
    },
    "Tree": {
      "background": "background",
      "foreground": "foreground",
      "rowHeight": 22,
      "selectionBackground": "selectionBackground",
      "selectionForeground": "foreground"
    }
  },
  "icons": {
    "ColorPalette": {
      "Blue": "#58a6ff",
      "Green": "#56d364",
      "Orange": "#f0883e",
      "Red": "#ff7b72",
      "Yellow": "#d29922"
    }
  }
}
